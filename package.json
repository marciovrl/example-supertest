{
  "name": "supertest-example",
  "version": "3.0.0",
  "description": "Example API testing using Supertest",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint --fix \"src/**\"",
    "test-feature": "mocha-parallel-tests ./src/test/steps/ --timeout 10000 --reporter mochawesome",
    "test-contract": "mocha-parallel-tests ./src/test/contracts/ --timeout 10000 --reporter mochawesome",
    "test": "mocha-parallel-tests ./src/test/* --timeout 10000 --reporter mochawesome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marciovrl/supertest.git"
  },
