  },
  "homepage": "https://github.com/marciovrl/supertest#readme",
  "dependencies": {
    "@hapi/joi": "16.1.7",
    "chai": "4.2.0",
    "mocha": "6.2.1",
    "mocha-parallel-tests": "2.2.2",
    "mochawesome": "4.1.0",
    "supertest": "4.0.2"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0"
